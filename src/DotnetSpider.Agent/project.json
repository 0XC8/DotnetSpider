  },
  "publishOptions": {
    "includeFiles": [ "config.ini", "nlog.config" ]
  },
  "dependencies": {
    "Microsoft.Extensions.Configuration": "1.0.0",
    "Microsoft.Extensions.Configuration.Abstractions": "1.0.0",
