    "Microsoft.Extensions.Configuration.Ini": "1.0.0",
    "Newtonsoft.Json": "*",
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.0"
    },
    "NLog": "4.4.0-betaV15",
    "StackExchange.Redis": "1.1.604-alpha",
    "System.Text.Encoding.CodePages": "4.0.1"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": "portable-net45+win8+dnxcore50",
      "dependencies": {},
      "buildOptions": {
        "define": [
