    "includeFiles": [ "config.ini", "nlog.config" ]
  },
  "dependencies": {
    "DotnetSpider.Core": "0.0.9",
    "DotnetSpider.Extension": "0.0.9",
    "DotnetSpider.Redial": "0.0.9",
